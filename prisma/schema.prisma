// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Budget {
  id        Int      @id @default(autoincrement())
  userId    String
  label     String
  createdAt DateTime
  startDate DateTime
  endDate   DateTime

  @@map("budgets")
}
